{
  "name": "ai-perf-scan",
  "version": "1.0.0",
  "description": "AI-powered performance scanner for React Native and React apps",
  "main": "dist/cli.js",
  "bin": {
    "ai-perf-scan": "./bin/ai-perf-scan.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --dts",
    "dev": "tsup src/cli.ts --format cjs --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "performance",
    "react-native",
    "react",
    "scanner",
    "ai",
    "optimization",
    "bundle-size",
    "cli"
  ],
  "author": "MMGAbdelhay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MMGAbdelhay/ai-perf-scan"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "openai": "^4.20.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
